NODE_ENV=development
APP_ENV=local
SERVICE_NAME=premiumbonus-integration
PORT=3000

RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX=60
ALLOWED_ORIGINS=

LOG_LEVEL=info
TRACING_ENABLED=false

PREMIUMBONUS_API_BASE_URL=https://api.premiumbonus.example
PREMIUMBONUS_TOKEN=replace-me

REDIS_HOST=127.0.0.1
REDIS_PORT=6379
REDIS_USERNAME=
REDIS_PASSWORD=
REDIS_TLS=false
REDIS_KEY_PREFIX=pb:

POSTGRES_CONNECTION_STRING=postgresql://user:password@localhost:5432/premiumbonus
POSTGRES_SCHEMA=public

QUEUE_NAME=purchases
QUEUE_ATTEMPTS=3
QUEUE_BACKOFF_DELAY_MS=2000

SECRET_PROVIDER=env

VAULT_ADDR=
VAULT_ROLE_ID=
VAULT_SECRET_ID=
VAULT_SECRET_PATH=
VAULT_TOKEN=
VAULT_NAMESPACE=

AWS_REGION=
AWS_SECRET_ID=
